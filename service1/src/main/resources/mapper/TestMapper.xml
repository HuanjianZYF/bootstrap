<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.programer.zyf.code.mapper.TestMapper">

    <select id="listCoupon" resultType="cn.programer.zyf.code.domain.CouponDO">
        select
        coupon_id  as id,
        coupon_name as couponName
        from coupon
        order by id desc
        limit 10
    </select>
</mapper>